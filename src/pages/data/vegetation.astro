---
import Layout from "../../layouts/Layout.astro";
---

<Layout title="Vegetation Data - EcoWatch">
  <div id="vegetation-root"></div>
</Layout>

<script>
  import { VegetationPage } from "../../components/data/VegetationPage";
  import React from "react";
  import { createRoot } from "react-dom/client";

  const container = document.getElementById("vegetation-root");
  if (container) {
    const root = createRoot(container);
    root.render(React.createElement(VegetationPage));
  }
</script>